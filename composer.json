		"tommcdo/kohana-twig" : "dev-master",
		"aptoma/twig-markdown" : "dev-master",
		"league/commonmark" : "0.8.*@dev",
		"league/oauth2-facebook" : "^1.0@dev",
		"league/oauth1-client" : "dev-master",
		"swiftmailer/swiftmailer": "^5.4",
		"psr/log": "^1.0",
		"rollbar/rollbar": "^0",
		"stephanecoinon/papertrail": "^1.0",
		"monolog/monolog": "^1.18"
	},
	"require-dev" : {
		"phing/phing" : "dev-master",
		"heroku/heroku-buildpack-php": "^90.0"
	},
	"minimum-stability" : "dev",
	"extra" : {
		"installer-paths" : {
			"vendor/{$vendor}/{$name}" : [
				"type:kohana-module"
			]
		}
